{
  "name": "conference-manager",
  "version": "2.3.0",
  "description": "A website and server to schedule, view, and manage conference room bookings for the 525 Chestnut St. office building",
  "scripts": {
    "dev": "concurrently 'dotenv -v NODE_ENV='development' -v PORT='5000' -e .env npm run server-dev' 'dotenv -e .env npm run static'",
    "server-dev": "nodemon src/server",
    "server": "node src/server",
    "static": "vite dev",
    "genusers": "node src/server/util/genUsers.js",
    "build": "vite build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/exoRift/conference-manager.git"
  },
  "author": "exoRift",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/exoRift/conference-manager/issues"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [
    "react",
    "web",
    "website",
    "api",
    "nodejs",
    "node",
    "js"
  ],
  "dependencies": {
    "@polka/send-type": "^0.5.2",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "ejs": "^3.1.6",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.3",
    "mass-require": "^1.0.0",
    "nodemailer": "^6.5.0",
    "pg": "^8.5.1",
    "polka": "^0.5.2",
    "query-string": "^7.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "sirv": "^2.0.2"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^2.2.0",
    "autoprefixer": "^10.4.13",
    "concurrently": "^6.0.0",
    "dotenv-cli": "^4.1.1",
    "eslint": "^8.27.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-config-standard": "^17.0.0",
    "nodemon": "^2.0.7",
    "vite": "^3.2.3",
    "vite-plugin-svgr": "^2.2.2"
  }
}
